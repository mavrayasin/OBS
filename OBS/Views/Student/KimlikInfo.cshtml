@model List<OBS.Models.Kimlik>
@{
    ViewBag.Title = "KimlikInfo";
    Layout = "~/Views/Shared/_StudentLayout.cshtml";
}



<div class="box">
    <div class="box-header with-border">
        <h3 class="box-title">Kimlik Bilgilerim</h3>
    </div>

<div class="box-body">
    <table class="table table-bordered">
        <tr>
            <th style="width: 10px">ID</th>
            <th>AD</th>
            <th>SOYAD</th>
            <th>TCNO</th>
            <th>DOĞUMYERİ</th>
            <th>DOĞUM TARİHİ</th>
           
        </tr>
        @foreach (var item in Model)
        {
    <tr>
        <td>@item.ID</td>
        <td>@item.Ad</td>
        <td>@item.Soyad</td>
        <td>@item.TcNo</td>
        <td>@item.DogumYeri</td>
        <td>@item.DogumTarihi</td>
    </tr>
        }



    </table>
</div>

